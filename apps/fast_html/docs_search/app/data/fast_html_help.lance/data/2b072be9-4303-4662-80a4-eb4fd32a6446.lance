from fasthtml.common import*exception_handlers={404: lambda req, exc: Titled("404: I don't exist!"),418: lambda req, exc: Titled("418: I'm a teapot!")}app, rt = fast_app(exception_handlers=exception_handlers)@rt('/')def get():return (Titled("Home page", P(A(href="/oops")("Click to generate 404 error"))))serve()
